{
  "name": "json-in-html",
  "version": "0.0.1",
  "description": "VSCode extension that supports JSON inside script tags and HTML attributes.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "publisher": "andersonbruceb",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/json-in-html.git"
  },
  "keywords": [
    "web-components",
    "import-maps",
    "web components",
    "custom-elements",
    "custom elements",
    "import maps",
    "json",
    "json-in-html",
    "import map",
    "import-map"
  ],
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "source.json.embedded",
        "path": "./syntax/script-json.embedded.json",
        "injectTo": [
          "text.html"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.json.html": "json"
        }
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/bahrus/json-in-html/issues"
  },
  "homepage": "https://github.com/bahrus/json-in-html#readme"
}
